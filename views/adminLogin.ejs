    <body class="text-center">
    
        <main class="form-signin">
            
            <div class="alert alert-danger" role="alert" id="login" style="display: none;">Please enter correct credentials and login again</div>
            <form action="/login" method="POST">
                <img class="logo-img" src="images/logo.png">
                <h1 class="h3 mb-3 fw-normal">Login</h1>
    
                
                <div class="form-floating">
                    <input name="username" type="username" class="form-control" id="floatingPassword" placeholder="Password">
                    <label for="floatingInput">Username</label>
                </div>
                <div class="form-floating">
                    <input name="password" type="password" class="form-control" id="floatingPassword" placeholder="Password">
                    <label for="floatingInput">Password</label>
                </div>
    
                <button class="w-100 btn btn-lg btn-primary" type="submit">Check Dashboard</button>
                <p class="mt-5 mb-3 text-muted">&copy;Value Wealth 2021</p>
            </form>
            <% if (loginError!=null) { %>
                <script>
                    function loginError()
                    {
                       document.getElementById('login').style.display="block";
                   
                    }
                    loginError();
                   </script>
               <% } else { %>
                   <script>
                       function correctLogin()
                   {
                       document.getElementById('login').style.display="none";
                   }
                   correctLogin();
                   </script>
                   
                   
                   <%}%>
        </main>
    
    
    
    </body>
    
